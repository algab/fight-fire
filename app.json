{
  "expo": {
    "name": "Fight Fire",
    "slug": "fight-fire",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D90B0B"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "androidStatusBar": {
      "backgroundColor": "#D90B0B",
      "barStyle": "light-content"
    },
    "android": {
      "package": "br.com.fightfire"
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
